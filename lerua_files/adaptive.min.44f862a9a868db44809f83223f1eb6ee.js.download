        $help.parent().toggleClass('has-success', !isError);
        $help.parent().toggleClass('has-error', isError);
        isError ? $help.show() : $help.hide();
        if (isError) {
            $help.html(errorText);
            $(Header.resetPasswordSettings.button).attr('disabled', 'disabled');
        }
    };
};

